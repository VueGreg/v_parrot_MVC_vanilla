{
    "prefer-stable": true,
    "require": {
        "vlucas/phpdotenv": "^4.2.0"
    },
    "autoload": {
        "psr-4": {
            "Utils\\": "app/Utils",
            "Models\\": "app/Models",
            "Router\\": "Router",
            "Controllers\\": "app/Http/Controllers",
            "Request\\": "app/Http/Request",
            "Actions\\": "app/Actions",
            "Exceptions\\": "app/Exceptions",
            "Middleware\\": "app/Http/Middleware"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
